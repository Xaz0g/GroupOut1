<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/tom"
    tools:context="com.example.marietopphem.groupout1.Home">


    <Button
        android:id="@+id/button2"
        android:layout_width="89dp"
        android:layout_height="67dp"
        android:background="@android:color/transparent"
        android:text="Hem"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="504dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="106dp"
        android:layout_height="65dp"
        android:background="@android:color/transparent"
        android:text="Inställningar"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="280dp"
        tools:layout_editor_absoluteY="504dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="99dp"
        android:layout_height="66dp"
        android:background="@android:color/transparent"
        android:text="Skapa Aktivitet"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="181dp"
        tools:layout_editor_absoluteY="504dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="90dp"
        android:layout_height="67dp"
        android:background="@android:color/transparent"
        android:text="Hitta Aktivitet"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="91dp"
        tools:layout_editor_absoluteY="504dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif"
        android:text="Kommande aktiviteter"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="154dp"
        app:layout_constraintHorizontal_bias="0.502"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1" />


    <TextView
        android:id="@+id/textView3"
        android:layout_width="237dp"
        android:layout_height="54dp"
        android:text='Du har ännu inga bokade aktiviteter. Klicka på "Hitta aktivitet" för att delta i en, eller skapa en egen!'
        android:textAlignment="center"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/tabHost"
        app:layout_constraintVertical_bias="0.019"
        app:layout_constraintHorizontal_bias="0.503"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1" />

</android.support.constraint.ConstraintLayout>
