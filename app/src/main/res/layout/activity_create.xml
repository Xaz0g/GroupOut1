<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context="com.example.marietopphem.groupout1.Create">

    <Button
        android:id="@+id/button15"
        android:layout_width="89dp"
        android:layout_height="67dp"
        android:background="@android:color/transparent"
        android:text="HEM"
        android:textColor="@android:color/white"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="504dp" />

    <Button
        android:id="@+id/button16"
        android:layout_width="90dp"
        android:layout_height="67dp"
        android:background="@android:color/transparent"
        android:text="HITTA AKTIVITET"
        android:textColor="@android:color/white"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="91dp"
        tools:layout_editor_absoluteY="504dp" />

    <Button
        android:id="@+id/button17"
        android:layout_width="99dp"
        android:layout_height="66dp"
        android:background="@android:color/transparent"
        android:text="SKAPA AKTIVITET"
        android:textColor="@android:color/white"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="181dp"
        tools:layout_editor_absoluteY="504dp" />

    <Button
        android:id="@+id/button18"
        android:layout_width="106dp"
        android:layout_height="65dp"
        android:background="@android:color/transparent"
        android:text="INSTÄLLNINGAR"
        android:textColor="@android:color/white"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="280dp"
        tools:layout_editor_absoluteY="504dp" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Namn på aktivitet"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="48dp"
        android:layout_height="44dp"
        app:srcCompat="@android:drawable/ic_menu_today"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="129dp" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="48dp"
        android:layout_height="44dp"
        app:srcCompat="@android:drawable/ic_menu_recent_history"
        tools:layout_editor_absoluteX="133dp"
        tools:layout_editor_absoluteY="129dp" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="48dp"
        android:layout_height="44dp"
        app:srcCompat="@android:drawable/ic_menu_recent_history"
        tools:layout_editor_absoluteX="255dp"
        tools:layout_editor_absoluteY="129dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="185dp"
        android:layout_height="22dp"
        android:text="Kategori - dropdown"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="193dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="185dp"
        android:layout_height="22dp"
        android:text="Plats - dropdown"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="75dp" />

    <EditText
        android:id="@+id/editText6"
        android:layout_width="139dp"
        android:layout_height="41dp"
        android:ems="10"
        android:hint="Min deltagare"
        android:inputType="number"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="225dp" />

    <EditText
        android:id="@+id/editText9"
        android:layout_width="139dp"
        android:layout_height="41dp"
        android:ems="10"
        android:hint="Max deltagare"
        android:inputType="number"
        tools:layout_editor_absoluteX="229dp"
        tools:layout_editor_absoluteY="225dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_menu_myplaces"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="225dp" />

    <Spinner
        android:id="@+id/platsSpinner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteY="280dp"
        tools:layout_editor_absoluteX="210dp">
        android:prompt="@string/Plats"
        android:entries="array/places"
    </Spinner>

</android.support.constraint.ConstraintLayout>
